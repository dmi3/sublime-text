%YAML 1.2
---
name: Log
file_extensions: [log]
scope: text

#http://stackoverflow.com/a/21914803

contexts:
  main:
    - match: \b[\w\.]*(Timeout|Exception|Error) # ‚Üê Add words to highlight here
      scope: invalid

    - match: \[(ALL|TRACE|DEBUG|INFO|WARN|ERROR|FATAL)\]
      scope: constant.language

    - match: \[.*?\]
      scope: comment

    - match: (\d{4}-\d{2}-\d{2}|\d{2}:\d{2}:\d{2},\d{3}) # Date/Time
      scope: string
